{
    "name": "dramatis-personae-backend",
    "version": "1.3.2",
    "description": "Backend for the Dramatis Personae project",
    "main": "index.js",
    "scripts": {
        "start": "ts-node src/index.ts",
        "test": "npm run typecheck && npm run lint && npm run prettier",
        "typecheck": "tsc --noEmit",
        "lint": "eslint \"src/**/*.ts{,x}\"",
        "lint:fix": "eslint \"src/**/*.ts{,x}\" --fix",
        "prettier": "prettier --check .",
        "prettier:fix": "prettier --write ."
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FrederikGoovaerts/Dramatis-Personae.git"
    },
    "author": "Frederik Goovaerts",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/FrederikGoovaerts/Dramatis-Personae/issues"
    },
    "devDependencies": {
        "@types/express": "^4.17.12",
        "@types/node": "^14.0.0",
        "@types/pg": "^8.6.0",
        "@typescript-eslint/eslint-plugin": "^4.28.1",
        "@typescript-eslint/parser": "^4.28.1",
        "eslint": "^7.30.0",
        "eslint-config-prettier": "^8.3.0",
        "prettier": "^2.3.2",
        "ts-node": "^10.0.0",
        "typescript": "^4.3.5"
    },
    "dependencies": {
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "pg": "^8.6.0",
        "postgres-migrations": "^5.2.0"
    }
}
